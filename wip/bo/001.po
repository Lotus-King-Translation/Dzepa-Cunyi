msgctxt "341fc2161c4146ab84d80f54f219668b"
msgid "། གང་ གིས་ དང་པོ་␣ར་ བྱང་ཆུབ་ ཐུགས་བསྐྱེད་ ནས །"
msgstr ""

msgctxt "48b8c85ae45e4dbd995559350e06be0b"
msgid "། བསོད་ནམས་ ཡེ་ཤེས་ ཚོགས་ གཉིས་ རྫོགས་ མཛད་ ཅིང༌ །"
