msgctxt "18c14c4c8ca740b2ae637c1fad9b9a7d"
msgid "། དུས་ འདི་␣ར་ མཛད་པ་ རྒྱ་ ཆེན་ འགྲོ་བ་ ཡི །"
msgstr ""

msgctxt "a0677edc408147a3877b8d58b27cccff"
msgid "། མགོན་ གྱུར་ ཁྱོད་ ལ་ བདག་ གིས་ བསྟོད་པ་␣ར་ བགྱི །"
msgstr ""

msgctxt "26d4e39aaedd4443bf2cf099c77806c8"
msgid "། ལྷ་ རྣམས་ འདུལ་ མཛད་ འདུལ་བ་␣འི་ དུས་མཁྱེན་ ནས །"
msgstr ""

msgctxt "8ca505ea711c46cbbf8f3b4a85cfe1f6"
msgid "། ལྷ་ ལ་␣ས་ བབས་ ནས་ གླང་ཆེན་ ལྟར་ གཤེགས་ ཏེ །"
msgstr ""

msgctxt "bb4ea52ec03f4f55bd0ab882dd703f3d"
msgid "། རིགས་ ལ་ གཟིགས་ ནས་ ལྷ་མོ་ སྒྱུ་འཕྲུལ་ གྱི །"
msgstr ""

msgctxt "ad4360822a574be18c1a66a8173c32c2"
msgid "། ལྷུམས་ སུ་ ཞུགས་པ་␣ར་ མཛད་ ལ་ ཕྱག་ འཚལ་ ལོ །"
msgstr ""

msgctxt "b12d3297b33c4083a99f2bef59803b09"
msgid "། ཟླ་བ་ བཅུ་ རྫོགས་ ཤཱཀྱའི་ སྲས་པོ་ ནི །"
msgstr ""

msgctxt "9bc406fc88794348a8f9823560dc7164"
msgid "། བཀྲ་ཤིས་ ལུམྦིའི་ ཚལ་ དུ་ བལྟམས་པ་␣འི་ ཚེ །"
msgstr ""

msgctxt "24ca83353ad34fd5a6556cfc539f4f5e"
msgid "། ཚངས་ དང་ བརྒྱ་བྱིན་ གྱིས་ བཏུད་ མཚན་ མཆོག་ ནི །"
msgstr ""

msgctxt "6660c665781e4de0bfbe577ddf9ea216"
msgid "། བྱང་ཆུབ་ རིགས་ སུ་ ངེས་ ལ་ ཕྱག་ འཚལ་ ལོ །"
msgstr ""

msgctxt "567105e2bed3491094b192f0137ebcb6"
msgid "། གཞོན་ནུ་ སྟོབས་ལྡན་ མི་ ཡི་ སེང་གེ་ དེ་␣ས །"
msgstr ""

msgctxt "04bc09273cc8416fac7259bfd89752a2"
msgid "། ཨཾ་ ག་ མ་ ག་ དྷར་ ནི་ སྒྱུ་རྩལ་ བསྟན །"
msgstr ""

msgctxt "6c8634d30b7544deae0f13d0a39a9905"
msgid "། སྐྱེ་བོ་ དྲེགས་པ་ ཅན་ རྣམས་ ཚར་ བཅད་ ནས །"
msgstr ""

msgctxt "9d37a9fc0ad4405ca16dd3ed8586fe41"
msgid "། འགྲན་ཟླ་ མེད་པ་␣ར་ མཛད་ ལ་ ཕྱག་ འཚལ་ ལོ །"
msgstr ""

msgctxt "40441d9c1f93424898595562f29d0da5"
msgid "། འཇིག་རྟེན་ ཆོས་ དང་ མཐུན་པ་␣ར་ བྱ་བ་ དང༌ །"
msgstr ""

msgctxt "9529e65d3af34f9b8ed550edc5d82666"
msgid "། ཁ་ན་མ་ཐོ་ སྤང་ ཕྱིར་ བཙུན་མོ་ ཡི །"
