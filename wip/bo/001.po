msgctxt "6a24caa77cbb4095aaf0fc05bcb3bb89"
msgid "། བསྟན་པ་ རྒྱས་པ་␣ར་ མཛད་ ལ་ ཕྱག་ འཚལ་ ལོ །"
msgstr ""

msgctxt "6d0c438ccab94b478d77ff9bd86566d0"
msgid "། ལེ་ལོ་ ཅན་ རྣམས་ ཆོས་ ལ་ བསྐུལ་ བྱ་␣འི་ ཕྱིར །"
msgstr ""

msgctxt "b8ac6212c9744a90ae25b8ee69e04eff"
msgid "། རྩྭ་མཆོག་ གྲོང་ གི་ ས་གཞི་ གཙང་མ་ རུ །"
