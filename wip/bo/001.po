msgctxt "fb662f6390c6496aa9d89b20215f6c2d"
msgid "། དཀའ་བ་ སྤྱད་ མཛད་ བརྩོན་འགྲུས་ མཐ་␣ར་ ཕྱིན་པ་␣ས །"
msgstr ""

msgctxt "5209ff2bb87840c793ea549be37264cb"
msgid "། བསམ་གཏན་ མཆོག་ བརྙེས་ མཛད་ ལ་ ཕྱག་ འཚལ་ ལོ །"
msgstr ""

msgctxt "571b6eac216843ce8e35ff232f4153af"
msgid "། ཐོག་མ་ མེད་ ནས་ འབད་པ་ དོན་ཡོད་ ཕྱིར །"
