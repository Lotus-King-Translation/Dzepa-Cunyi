msgctxt "141708dbafb0482b8254810a3623acfa"
msgid "། འཁོར་ དང་ ལྡན་ མཛད་ ཐབས་ ལ་ མཁས་པ་ ཡིས །"
msgstr ""

msgctxt "31e877a6e0904ec6a18e83e3abd7e181"
msgid "། རྒྱལ་སྲིད་ སྐྱོང་བ་␣ར་ མཛད་ ལ་ ཕྱག་ འཚལ་ ལོ །"
msgstr ""

msgctxt "f5af07dcf0d14c0fad0fb743d1e62ebf"
msgid "། འཁོར་བ་␣འི་ བྱ་བ་␣ར་ སྙིང་པོ་ མེད་ གཟིགས་ ནས །"
msgstr ""

msgctxt "b186110e2375482db02e1b74d491fa64"
msgid "། ཁྱིམ་ ནས་ བྱུང་ སྟེ་ མཁའ་ ལ་ གཤེགས་ ནས་ ཀྱང༌ །"
msgstr ""

msgctxt "71403833eae0401fbdf51787d1d28921"
msgid "། མཆོད་རྟེན་ རྣམ་དག་ དྲུང་ དུ་ ཉིད་ ལ་␣ས་ ཉིད །"
