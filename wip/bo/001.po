msgctxt "79b5f563777d432680d5176f5363823b"
msgid "། ཐུགས་རྗེ་␣ས་ འགྲོ་ ལ་ མྱུར་ དུ་ གཟིགས་ ནས་ ནི །"
msgstr ""

msgctxt "4c7a13e128c84e80a1524e02e49e06b1"
msgid "། ཝཱ་ རཱ་ ཎ་ སཱི་ ལ་སོགས་ གནས་མཆོག་ ཏུ །"
