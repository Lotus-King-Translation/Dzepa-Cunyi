msgctxt "addabb2f4a0741d59d9d272fa8f60f2f"
msgid "། རབ་ཏུ་ བྱུང་བ་␣ར་ མཛད་ ལ་ ཕྱག་ འཚལ་ ལོ །"
msgstr ""

msgctxt "2954729abc274a3b868dffad70616e59"
msgid "། བརྩོན་པ་␣ས་ བྱང་ཆུབ་ སྒྲུབ་པ་␣ར་ དགོངས་ ནས་ ནི །"
