msgctxt "745a6a766f33427688b311afd90d555f"
msgid "། གཞན་ གྱིས་ རྒོལ་བ་ ངན་པ་ ཚར་ བཅད་ ཕྱིར །"
msgstr ""

msgctxt "45926b6a82fa4b8580346999099a7bab"
msgid "། མུ་སྟེགས་ སྟོན་པ་ དྲུག་ དང་ ལྷས་བྱིན་ སོགས །"
