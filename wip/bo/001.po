msgctxt "fa6fdff7d2ca45a2987b594dbc1329c2"
msgid "། འཁོར་མོ་ འཇིག་ གི་ ཡུལ་ དུ་ བདུད་ རྣམས་ བཏུལ །"
msgstr ""

msgctxt "c73008aedc11479db983167eb436f601"
msgid "། ཐུབ་པ་ གཡུལ་ལས་ རྒྱལ་ ལ་ ཕྱག་ འཚལ་ ལོ །"
